<%- include('_header.ejs') %>

    <div class="album py-5 bg-light">
        <div class="container">
            <div class="row">
                <% appointments.forEach(appointment => { %>
                <div class="col-md-4">
                    <div class="card mb-4 box-shadow">
                        <div class="card-body">
                            <h4 class="card-title"><%= appointment.firstname + " " + appointment.lastname %></h4>
                            <p class="card-text"><%= appointment.phonenumber %></p>
                            <p class="card-text"><%= appointment.course %></p>
                            <p class="card-text"><%= appointment.avaliability%></p>
                            <div class="d-flex justify-content-between align-items-center">
                            </div>
                        </div>
                    </div>
                </div>
                <% }) %>
            </div>
        </div>
    </div>
</body>
<%- include('_footer.ejs') %>
</html>